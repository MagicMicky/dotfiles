[user]
  name = MagicMicky
  email = micky0304@gmail.com
[push]
  default = simple
[alias]
  st = status
  undo = git reset --soft HEAD^
  lol = log --graph --decorate --pretty=oneline --abbrev-commit
  lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
  open = "!f(){ open $(git config remote.origin.url | sed \"s#git@\\(.*\\):\\(.*\\).git#https://\\1/\\2/tree/$(git symbolic-ref --quiet --short HEAD)/$(git rev-parse --show-prefix)$1#\"); }; f"

[color]
  branch = auto
  diff = auto
  interactive = auto
  status = auto
[url "git@github.com:"]
  insteadOf = https://github.com/
